                    </#if> 
                    </#if>
                </div>
                </#list>
                </#if>
            </div>
        </div>

        <!-- 行程介绍 开始 -->
        <div class="product-detail-instance pd-introduction clearfix">
            <div class="detail_content_tit">
                <span><i class="detail_icon detail_icon_xcjs"></i>行程介绍</span>
            </div>
            <#if product.prodLineRouteList?? && product.prodLineRouteList?size gt 0>
            <div class="clearfix">
                <ul class="instance_tab fl">
                    <#list product.prodLineRouteList as prodLineRoute>
                        <#if prodLineRoute.lineRouteId == "${routeId}">
                             <li class="active">${prodLineRoute.routeName}<i class="group_icon icon_gou"></i></li>
                        </#if>
                    </#list>
                </ul>
            </div>
              <#--循环生成行程明细   （行程天数，行程特色，出行时间，线路概况）开始-->
               <#include "/routeintroductionInfo.ftl">
              <#--循环生成行程明细   （行程天数，行程特色，出行时间，线路概况）结束-->
            </#if>

            <!-- 交通 -->
            <#if trafficList && trafficList?size gt 0 && product.packageType == 'SUPPLIER'>
            <#list trafficList as trafficMap>
            <div class="traffic-type clearfix" id="traffic-type">
                <ul>
                    <li class="active">交通${trafficMap['titleIndex']}<i class="arrow"></i></li>
                </ul>
            </div>
            <div class="traffic-container" id="traffic-container">

                <!-- 交通1 -->
                <div id="traffic1-${trafficMap['titleIndex']}-container" class="traffic-instance" style="display: block;">
                    <dl class="traffic-item clearfix">
                        <dt class="title">去程</dt>
                        <#if trafficMap['toMap']['toList'] && trafficMap['toMap']['toList']?size gt 0>
                        <dd class="outer-container">
                            <div class="container clearfix">
                                <ul>
                                    <#assign changeSize = trafficMap['toMap']['toList']?size>
                                    <#list trafficMap['toMap']['toList'] as trafficForRouteDetailVO>
                                    <#if trafficMap['toType'] == "FLIGHT">
                                    <#if trafficForRouteDetailVO.changeflag>
                                    <div class="traffic-item-middle clearfix">
                                            <div class="traffic-item-middle-line"></div>
                                            <div class="traffic-item-middle-content clearfix">
                                                <div class="traffic-item-area">
                                                    <p>
                                                        <i>转</i>${trafficForRouteDetailVO.changeTrains}
                                                    </p>
                                                </div>
                                            </div>

                                        </div>
                                    <#else>
                                        <li class="traffic-item-plane first-li traffic-item-plane-transfer">
                                        <div class="traffic-item-top clearfix">
                                            <div class="traffic-item-icon"></div>
                                            <div class="traffic-item-number">
                                                <p>${trafficForRouteDetailVO.belongToCompany}</p>
                                                <p>${trafficForRouteDetailVO.number}</p>
                                            </div>
                                            <div class="traffic-item-clock">
                                                <p>${trafficForRouteDetailVO.startTime}</p>
                                                <p><i>${trafficForRouteDetailVO.endTime}</i> <em>+1天</em></p>
                                            </div>
                                            <div class="traffic-item-area">
                                                <#if changeSize gt 1 && trafficForRouteDetailVO_index !=0>
                                                     <p>&nbsp;</p>
                                                <#else>
                                                     <p>${trafficForRouteDetailVO.startAirport}</p>
                                                </#if>

                                                <#if changeSize gt 1 && trafficForRouteDetailVO_index !=changeSize-1>
                                                     <p>&nbsp;</p>
                                                <#else>
                                                     <p>${trafficForRouteDetailVO.arriveAirport}</p>
                                                </#if>
                                            </div>
                                            <div class="traffic-item-time">
                                                 ${trafficMap['toTotalHour']}
                                            </div>
                                            <div class="traffic-item-type">
                                                 ${trafficForRouteDetailVO.cabin}
                                            </div>
                                        </div>
                                    </li>
                                    </#if>
                                    <#elseif trafficMap['toType'] == "TRAIN">
                                    <li class="traffic-item-train clearfix">
                                        <div class="traffic-item-icon"></div>
                                        <div class="traffic-item-number">
                                            <p>${trafficForRouteDetailVO.number}</p>
                                        </div>
                                        <div class="traffic-item-clock">
                                            <p>${trafficForRouteDetailVO.startTime}</p>
                                            <p>${trafficForRouteDetailVO.endTime}</p>
                                        </div>
                                        <div class="traffic-item-area">
                                            <p>${trafficForRouteDetailVO.fromDistrict}</p>
                                            <p>${trafficForRouteDetailVO.toDistrict}
                                               <#if trafficForRouteDetailVO.stopFlag == 'Y'>
                                                    <em>经停</em>
                                               </#if>
                                            </p>
                                        </div>
                                        <div class="traffic-item-time">
                                              ${trafficForRouteDetailVO.costTime}
                                        </div>
                                        <div class="traffic-item-type">
                                             ${trafficForRouteDetailVO.seatType}
                                        </div>
                                    </li>
                                    
                                    <#elseif trafficMap['toType'] == "BUS">
                                         <li class="traffic-item-bus clearfix">
                                            <div class="traffic-item-icon"></div>
                                            <div class="traffic-item-text">
                                                 <p>上车点：${trafficForRouteDetailVO.address}</p>
                                                 <p>发车时间：${trafficForRouteDetailVO.startTime}</p>
                                           </div>
                                       </li>
                                    </#if>
                                 </#list>
                                </ul>
                            </div>
                        </dd>
                        </#if>
                    </dl>
                    <dl class="traffic-item clearfix">
                       <!-- 返程交通 -->
                       
                       <dt class="title">返程</dt>
                       <#if trafficMap['backMap']['backList'] && trafficMap['backMap']['backList']?size gt 0>
                        <dd class="outer-container">
                            <div class="container clearfix">
                                <ul>
                                   <#assign changeSize = trafficMap['backMap']['backList']?size>
                                   <#list trafficMap['backMap']['backList'] as trafficForRouteDetailVO>
                                   <#if trafficMap['backType'] == "FLIGHT">
                                    <#if trafficForRouteDetailVO.changeflag>
                                    <div class="traffic-item-middle clearfix">
                                            <div class="traffic-item-middle-line"></div>
                                            <div class="traffic-item-middle-content clearfix">
                                                <div class="traffic-item-area">
                                                    <p>
                                                        <i>转</i>${trafficForRouteDetailVO.changeTrains}
                                                    </p>
                                                </div>
                                            </div>
                                        </div>
                                    <#else>
                                        <li class="traffic-item-plane first-li traffic-item-plane-transfer">
                                        <div class="traffic-item-top clearfix">
                                            <div class="traffic-item-icon"></div>
                                            <div class="traffic-item-number">
                                                <p>${trafficForRouteDetailVO.belongToCompany}</p>
                                                <p>${trafficForRouteDetailVO.number}</p>
                                            </div>
                                            <div class="traffic-item-clock">
                                                <p>${trafficForRouteDetailVO.startTime}</p>
                                                <p><i>${trafficForRouteDetailVO.endTime}</i> <em>+1天</em></p>
                                            </div>
                                            <div class="traffic-item-area">
                                                <#if changeSize gt 1 && trafficForRouteDetailVO_index !=0>
                                                     <p>&nbsp;</p>
                                                <#else>
                                                     <p>${trafficForRouteDetailVO.startAirport}</p>
                                                </#if>

                                                <#if changeSize gt 1 && trafficForRouteDetailVO_index !=changeSize-1>
                                                     <p>&nbsp;</p>
                                                <#else>
                                                     <p>${trafficForRouteDetailVO.arriveAirport}</p>
                                                </#if>

                                            </div>
                                            
                                            <div class="traffic-item-time">
                                                 ${trafficMap['backTotalHour']}
                                            </div>
                                            <div class="traffic-item-type">
                                                 ${trafficForRouteDetailVO.cabin}
                                            </div>
                                        </div>
                                    </li>
                                    </#if>
                                      
                                    <#elseif trafficMap['toType'] == "TRAIN">
                                    <li class="traffic-item-train clearfix">
                                        <div class="traffic-item-icon"></div>
                                        <div class="traffic-item-number">
                                            <p>${trafficForRouteDetailVO.number}</p>
                                        </div>
                                        <div class="traffic-item-clock">
                                            <p>${trafficForRouteDetailVO.startTime}</p>
                                            <p>${trafficForRouteDetailVO.endTime}</p>
                                        </div>
                                        <div class="traffic-item-area">
                                            <p>${trafficForRouteDetailVO.fromDistrict}</p>
                                            <p>${trafficForRouteDetailVO.toDistrict}
                                               <#if trafficForRouteDetailVO.stopFlag == 'Y'>
                                                    <em>经停</em>
                                               </#if>
                                            </p>
                                        </div>
                                        <div class="traffic-item-time">
                                             ${trafficForRouteDetailVO.costTime}
                                        </div>
                                        <div class="traffic-item-type">
                                             ${trafficForRouteDetailVO.seatType}
                                        </div>
                                    </li>
                                    
                                    
                                    <#elseif trafficMap['toType'] == "BUS">
                                       <li class="traffic-item-bus clearfix">
                                            <div class="traffic-item-icon"></div>
                                            <div class="traffic-item-text">
                                                 <p>上车点：${trafficForRouteDetailVO.address}</p>
                                                 <p>发车时间：${trafficForRouteDetailVO.startTime}</p>
                                           </div>
                                       </li>
                                    </#if>
                                  </#list>
                                </ul>
                            </div>
                        </dd>
                        </#if>
                    </dl>
                </div>
                <!-- //traffic-instance -->
            </div>
             </#list>
            </#if> 
            <#-- 行程明细开始 -->
            <#include "/routePrintShowAllRouteTemplate.ftl">
            <#-- 行程明细结束 -->
        </div>        
        <!-- 行程介绍 结束 -->

        <!-- 费用说明 开始 -->
        <div class="product-detail-instance pd-details">
            <#list product.prodLineRouteList as prodLineRoute>
            <#if prodLineRoute.costInclude?exists || prodLineRoute.costExclude?exists>
            <div class="detail_content_tit">
                <span><i class="detail_icon detail_icon_fysm"></i>费用说明</span>
            </div>
            <div class="detail-instance-body">
                <#if prodLineRoute.costInclude?exists>
                <div class="detail-desc-item">
                    <div class="operate">费用包含</div>
                    <div class="detail">  
                        <#list prodLineRoute.costInclude?split("<br/>") as costInclude> 
                               <@func.addSpace costInclude 48/> 
                        </#list>

                </div>
                </#if>
                <#if prodLineRoute.costExclude?exists>
                <div class="detail-desc-item">
                    <div class="operate">费用不包含</div>
                    <div class="detail"> 
                         <#list prodLineRoute.costExclude?split("<br/>") as costExclude>
                                  <@func.addSpace costExclude 48/> 
                         </#list>
                    </div>
                </div>
               </#if>
            </div>
            </#if>
            </#list>
        </div>
        <!-- 费用说明 结束 -->

        <!-- 预订须知 开始 -->
        <div class="product-detail-instance">
            <div class="detail_content_tit">
                <span><i class="detail_icon detail_icon_ydxz"></i>预订须知</span>
            </div>
            <div class="detail-instance-body preorder-notice">
                <#if product.prodEcontract.groupType?exists && product.prodEcontract.groupType='COMMISSIONED_TOUR'>
                     <div class="declaration">本产品委托社为：${product.prodEcontract.groupSupplierName}，具体的旅游服务和操作由委托社提供。</div>
                <#elseif ("INNERLINE"==product.productType || "INNERSHORTLINE"==product.productType || "INNERLONGLINE"==product.productType || "INNER_BORDER_LINE"==product.productType)
                               &&"LVMAMA"==product.packageType&&product.bizCategory.categoryId == 15&&"SELF_TOUR"==product.prodEcontract.groupType>
                        <#if suppContractName?exists>
                    	 <div class="declaration">本产品由${suppContractName}及具有合法资质的旅行社提供相关服务。</div>
                    	 </#if>
                </#if>
                
                <#if productNoticeList && productNoticeList?size gt 0>
                <div class="notice">
                    <h6>公告</h6>
                    <#list productNoticeList as productNotice>
                        <p>${productNotice_index + 1}、  ${productNotice.content?replace("\n", "<br/>")}</p>
                    </#list>
                </div>
                </#if>
                
                <!-- 购物说明 开始 -->
                <div class="instance_list2_box" id="line_route_visa">
                  <#list product.prodLineRouteList as prodLineRoute>
                  <#if prodLineRoute.lineRouteId == "${routeId}">
                    <div class="detail-desc-item">
                        <#if prodLineRoute.shopingList?exists && prodLineRoute.shopingList?size gt 0>
                            <div class="operate">
                                                                          购物说明
                            </div>
                            <!-- 主要内容显示区域\\ -->
                            <div class="iframe-content">
                                <div class="p_box">
                                    <table class="p_table">
                                        <thead>
                                        <tr>
                                            <th width="65">行程天数</th>
                                            <th width="105">地点</th>
                                            <th width="100">购物场所</th>
                                            <th width="110">主要商品</th>
                                            <th width="70">最长停留时间</th>
                                            <th width="140">其他</th>
                                        </tr>
                                        </thead>
                                        <tbody>
                                        <#if prodLineRoute.shopingList?? && prodLineRoute.shopingList?size &gt; 0>
                                        <#list prodLineRoute.shopingList as shopingItem>
                                        <tr>
                                            <td>${shopingItem.nDays!''}</td>
                                            <td class="cell-pull-left">${shopingItem.address!''}</td>
                                            <td class="cell-pull-left">${shopingItem.detailName!''}</td>
                                            <td class="cell-pull-left">${shopingItem.detailValue!''} </td>
                                            <td>${shopingItem.stay!''} </td>
                                            <td class="cell-pull-left"><@func.addSpace shopingItem.other 14/></td>
                                        </tr>
                                        </#list>
                                        </#if>
                                        </tbody>
                                    </table>
                                </div><!-- div p_box -->
                            </div><!-- //主要内容显示区域 -->
                         </#if>
                    </div>
                    </#if>
                    </#list>
                </div>
                <!-- 购物说明 结束 -->
                
                <!-- 推荐项目 开始 -->
                <div class="instance_list2_box" id="line_route_visa">
                   <#list product.prodLineRouteList as prodLineRoute>
                   <#if prodLineRoute.lineRouteId == "${routeId}">
                    <div class="detail-desc-item">
                       <#if prodLineRoute.recommendList?exists && prodLineRoute.recommendList?size gt 0>
                        <div class="operate">
                                                                 推荐项目
                        </div>
                        <div class="iframe-content">
                            <div class="p_box">
                                <table class="p_table table_center">
                                    <thead>
                                    <tr>
                                        <th width="65">行程天数</th>
                                        <th width="105">地点</th>
                                        <th width="100">项目名称和内容</th>
                                        <th width="110">费用</th>
                                        <th width="70">时长</th>
                                        <th width="140">其他</th>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    <#if prodLineRoute.recommendList?? && prodLineRoute.recommendList?size &gt; 0>
                                    <#list prodLineRoute.recommendList as recommendItem>
                                    <tr>
                                        <td>${recommendItem.nDays!''}</td>
                                        <td class="cell-pull-left">${recommendItem.address!''}</td>
                                        <td class="cell-pull-left">${recommendItem.detailName!''} </td>
                                        <td class="cell-pull-left">${recommendItem.detailValue!''} </td>
                                        <td>${recommendItem.stay!''}</td>
                                        <td class="cell-pull-left">
                                            <#if recommendItem?? && recommendItem.other?? && recommendItem.other?length gt 0>
                                                 <@func.addSpace recommendItem.other 14/>
                                            <#else>
                                                 <@func.addSpace "特别说明：自费项目均是建议性项目，客人应本着“自愿自费”的原则酌情参加，如以上项目参加人数不足时，则可能无法成行或费用做相应调整。" 14/>
                                            </#if>
                                        </td>
                                    </tr>
                                    </#list>
                                    </#if>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </#if>
                    </div>
                    </#if>
                    </#list>
                </div>
                <!-- 推荐项目 结束 -->
                    
                <!-- 出行警示及说明 开始 -->
                <div class="detail-desc-item">
                    <#if prodProductPropMap && prodProductPropMap['warning']>
                    <div class="operate">出行警示及说明</div>
                    <div class="detail">
                         <#list prodProductPropMap['warning']?split("<br/>") as warning>
                                <@func.addSpace warning 50/>
                         </#list>
                    </div>
                    </#if>
                    <#if prodProductPropMap && prodProductPropMap['important']>
                    <div class="detail">
                        <#list prodProductPropMap['important']?split("<br/>") as important>
                               <@func.addSpace important 50/>
                         </#list>
                    </div>
                    </#if>
                    <#if prodProductPropMap && prodProductPropMap['change_and_cancellation_instructions']>
                    <div class="operate mt20">退改说明</div>
                    <div class="detail">
                         <#list prodProductPropMap['change_and_cancellation_instructions']?split("<br/>") as change_and_cancellation_instructions>
                               <@func.addSpace change_and_cancellation_instructions 50/>
                         </#list>
                    </div>
                    </#if>
                </div>
                <!-- 出行警示及说明 结束 -->
            </div>
        </div>
        <!-- 预订须知 结束 -->

        <p class="footTip">行程明细及费用说明最终以产品购买页面显示为准</p>
        <#if product.urlQR??>
        <div class="footEwm">
            <p>扫码订购，优惠多多</p>
            <p>产品ID：${product.productId}</p>
            <img src="${product.urlQR}" width="127" height="127">
        </div>
        </#if>
    </div>

</body>
</html>